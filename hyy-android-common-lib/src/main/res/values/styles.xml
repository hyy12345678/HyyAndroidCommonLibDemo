        <item name="android:maxHeight">@dimen/hyy_commlib_drop_down_list_footer_progress_bar_height</item>
    </style>

    <style name="hyy_commlib_drop_down_list_header_font_style">
        <item name="android:textColor">@color/hyy_commlib_drop_down_list_header_font_color</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
    </style>

    <style name="hyy_commlib_drop_down_list_header_second_font_style">
