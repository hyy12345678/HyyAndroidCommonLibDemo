<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="hyy_commlib_drop_down_list_header_progress_bar_style">
        <item name="android:minHeight">@dimen/hyy_commlib_drop_down_list_header_progress_bar_height</item>
        <item name="android:maxHeight">@dimen/hyy_commlib_drop_down_list_header_progress_bar_height</item>
    </style>

    <style name="hyy_commlib_drop_down_list_footer_progress_bar_style">
